{
  "name": "sendfire",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "test": "karma start ClientApp/test/karma.conf.js",
    "build:tsd": "webpack --config webpack.config.vendor.js --env.dev && webpack --config webpack.config.js --env.dev",
    "build:tsp": "webpack --config webpack.config.vendor.js -p --env.prod && webpack --config webpack.config.js -p --env.prod",
    "build:css": "node-sass ClientApp/app/scss/application.scss wwwroot/dist/application.css",
    "watch:css": "node-sass -w ClientApp/app/scss/application.scss wwwroot/dist/application.css",
    "watch:tsd": "webpack --config webpack.config.js --env.dev -w",
    "run:local": "concurrently \"export ASPNETCORE_ENVIRONMENT=Local && dotnet watch run\" \"yarn build:tsd && yarn watch:tsd\" \" yarn watch:css\""
  },
